<p id="notice"><%= notice %></p>

<h1>Main Screens</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @main_screens.each do |main_screen| %>
      <tr>
        <td><%= link_to 'Show', main_screen %></td>
        <td><%= link_to 'Edit', edit_main_screen_path(main_screen) %></td>
        <td><%= link_to 'Destroy', main_screen, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Main Screen', new_main_screen_path %>
